MODULE bee2057Time;

VAR

    S, T, F , ans: INTEGER;

BEGIN
    S :=  22;
    T :=  6;
    F :=  -2;

    ans := S + T + F;

    IF ans > 24 THEN
        ans := ans - 24

    ELSIF ans < 0 THEN
        ans := 24 - ans
    
    ELSIF ans < 24 THEN
        ans := ans
    END
END

END bee2057Time.compile